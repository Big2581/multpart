<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.big.dao.IUploadDAO">
	<insert id="insertbyUpload" parameterType="com.big.entity.Upload">
		insert into Upload (img,img_date) values(#{img},#{img_date})
	</insert>
	<select id="selectAll" resultType="com.big.entity.Upload">
		select * from upload
	</select>
</mapper>